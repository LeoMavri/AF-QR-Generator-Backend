{
    "name": "af-qr-code-generator-backend",
    "version": "1.2.0",
    "author": "",
    "description": "",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": ">=16.9.0"
    },
    "type": "module",
    "exports": [
        "./dist/index.js"
    ],
    "scripts": {
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "eslint . --fix --cache --ext .js,.jsx,.ts,.tsx",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "clean": "git clean -xdf --exclude=\"/config/**/*\"",
        "clean:dry": "git clean -xdf --exclude=\"/config/**/*\" --dry-run",
        "build": "tsc --project tsconfig.json",
        "start": "npm run build && node --enable-source-maps dist/index.js",
        "start:pm2": "npm run build && npm run pm2:start",
        "pm2:start": "pm2 start process.json",
        "pm2:stop": "pm2 stop process.json",
        "pm2:delete": "pm2 delete process.json"
    },
    "dependencies": {
        "@types/qrcode": "^1.5.5",
        "cron-parser": "^4.9.0",
        "dotenv": "^16.3.1",
        "express": "4.18.2",
        "express-promise-router": "4.1.1",
        "node-schedule": "2.1.1",
        "pg": "^8.11.3",
        "pino": "8.17.1",
        "pino-pretty": "10.3.0",
        "pm2": "^5.3.0",
        "qrcode": "^1.5.3",
        "reflect-metadata": "^0.2.1",
        "typeorm": "^0.3.17",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/node": "^20.10.5",
        "@types/node-schedule": "2.1.5",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "eslint": "^8.56.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-unicorn": "^49.0.0",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3"
    }
}
